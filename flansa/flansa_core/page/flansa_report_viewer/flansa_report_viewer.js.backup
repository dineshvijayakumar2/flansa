        $('body').append(lightboxHtml);
        
        // Store current state
        this.lightbox_images = images;
        this.lightbox_current_index = 0;
        
        console.log('üíæ Lightbox state stored:', {
            lightboxImages: this.lightbox_images,
            currentIndex: this.lightbox_current_index
