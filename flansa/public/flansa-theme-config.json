  "gradients": {
    "primary": "linear-gradient(135deg, var(--flansa-primary) 0%, var(--flansa-secondary) 100%)",
    "accent": "linear-gradient(135deg, var(--flansa-accent) 0%, var(--flansa-primary) 100%)",
    "light": "linear-gradient(135deg, var(--flansa-white) 0%, var(--flansa-gray-50) 100%)"
  },
  "shadows": {
    "sm": "0 1px 2px 0 rgba(0, 0, 0, 0.05)",
